{
  "name": "Isaac's New Tab",
  "version": "3.0.0",
  "description": "My custom new tab page",
  "manifest_version": 3,
  "minimum_chrome_version": "88.0.0.0",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
